
@{
    ViewBag.Title = "Erro404";
    Layout = "~/Views/Shared/_LayoutErros.cshtml";
}
<div class="preloader">
    <svg class="circular" viewBox="25 25 50 50">
        <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="2" stroke-miterlimit="10" />
    </svg>
</div>
<!-- Main wrapper  -->
<div class="error-page" id="wrapper">
    <div class="error-box">
        <div class="error-body text-center">
            <h1>404</h1>
            <h3 class="text-uppercase">Página não encontrada </h3>
            <p class="text-muted m-t-30 m-b-30">Por favor tente novamente mais tarde</p>
            <a class="btn btn-info btn-rounded waves-effect waves-light m-b-40" href="@Url.Action("Index","Home")">Voltar para o inicio</a>
        </div>
        <footer class="footer text-center fixed-bottom">&copy; @DateTime.Now.Year CEEM - Consumo de Energia Elétrica Monitorado.</footer>
    </div>
</div>

